   "source": [
    "def pow(a,b):\n",
    "    sum = 0\n",
    "    if b>a:\n",
    "        for i in range (a,b+1):\n",
    "            sum += i\n",
    "        return sum\n",
    "    else:\n",
    "        for i in range (b,a+1):\n",
    "            sum += i\n",
    "    return sum"
   ]
